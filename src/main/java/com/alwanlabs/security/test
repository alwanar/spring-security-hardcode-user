        if(!requestError.isEmpty()) {
            String s = String.join(",", requestError).toString();
            response.put("error", true);
            response.put("message", "field request " + s + " tidak ada");
            return response;
        }

        response.put("error", false);
        response.put("message", "success");

        return response;
